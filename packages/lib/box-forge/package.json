{
    "name": "@naomiarotest/lib-box-forge",
    "version": "0.0.50",
    "private": true,
    "license": "LGPL-3.0-or-later",
    "exports": {
        ".": "./src/index.ts"
    },
    "scripts": {
        "lint": "eslint \"**/*.ts\"",
        "clear": "rm -rf ./test/gen/*",
        "forge": "npx tsx test/schema.ts",
        "format": "prettier --write \"./test/gen/**/*.ts\" --ignore-path /dev/null",
        "gen": "npm run clear && npm run forge && npm run format",
        "test": "npm run gen && vitest run"
    },
    "dependencies": {
        "@naomiarotest/lib-box": "^0.0.50",
        "@naomiarotest/lib-dom": "^0.0.50",
        "@naomiarotest/lib-runtime": "^0.0.50",
        "@naomiarotest/lib-std": "^0.0.50",
        "ts-morph": "^25.0.1",
        "tsx": "^4.20.3"
    },
    "devDependencies": {
        "@naomiarotest/eslint-config": "^0.0.20",
        "@naomiarotest/typescript-config": "^0.0.20"
    }
}
